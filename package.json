{
  "name": "scrollmap",
  "description": "Show markers on the scroll bar",
  "repository": "https://github.com/asiloisad/pulsar-scrollmap",
  "keywords": [
    "scrollmap"
  ],
  "version": "5.3.0",
  "license": "MIT",
  "engines": {
    "atom": ">=1.100.0 <2.0.0"
  },
  "main": "./lib/main",
  "consumedServices": {
    "scrollmap": {
      "versions": {
        "1.0.0": "consumeScrollmap"
      }
    }
  },
  "providedServices": {
    "simplemap": {
      "versions": {
        "1.0.0": "provideSimplemap"
      }
    }
  },
  "dependencies": {
    "etch": "^0.14.1",
    "pulsar-select-list": "^1.0.0"
  },
  "configSchema": {
    "disabledLayers": {
      "type": "array",
      "default": [],
      "items": {
        "type": "string"
      },
      "description": "List of disabled layer names"
    }
  }
}
